											//Creates an indicator for each slide and sets the first slide and indicator to current
											function initialSetupForSliderWidget() {
												var slides = jQuery(".slide");
												var slideControls = jQuery(".slideInfo");
												n=slides.length;
												
												slides.hide();
												jQuery(slides[0]).css('opacity','1').show();
											
												jQuery(slideControls[0]).addClass("selected");
												jQuery(slideControls[0]).find(".carrot").show();
												
												autoRotateRight();
											}
											function showNewSlide(oldIndex, newIndex) {
												var slides=jQuery(".slide");
												newIndex = (newIndex + slides.size()) % slides.size();
												
												jQuery(".slideInfo.selected").removeClass("selected").find(".carrot").hide();
												
												jQuery(jQuery(".slideInfo")[newIndex]).addClass("selected").find(".carrot").show();
												
													jQuery(".slide").hide().css("opacity","0");
												
												jQuery(jQuery(".slide")[newIndex]).show().animate({
													opacity:1
												}, 500);
												
												currentSlide=newIndex;
											}
											function autoRotateRight() {
												clearTimeout(autoRotateTimer);
												autoRotateTimer = setTimeout(showSlideRight , AUTO_ROTATE_TIME);
											}
